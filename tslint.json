{
  "rules": {
    "adjacent-overload-signatures": true,
    "array-type": [
      true,
      "array"
    ],
    "arrow-parens": true,
    "class-name": true,
    "comment-format": [
      true,
      "check-space"
    ],
    "completed-docs": [
      true,
      "functions",
      "methods"
    ],
    "curly": true,
    "indent": [
      true,
      "spaces",
      4
    ],
    "jsdoc-format": [true, "check-multiline-start"],
    "new-parens": true,
    "no-any": true,
    "no-unused-expression": true,
    "no-duplicate-variable": true,
    "no-trailing-whitespace": true,
    "no-var-keyword": true,
    "only-arrow-functions": [
      true,
      "allow-declarations",
      "allow-named-functions"
    ],
    "quotemark": [
      true,
      "double",
      "avoid-escape",
      "avoid-template"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always",
        "named": "never"
      }
    ],
    "triple-equals": true,
    "prefer-for-of": true
  }
}